{
  "name": "node-backend-boilerplate",
  "version": "0.0.2",
  "description": "Boilerplate code to begin a Node.js ECMA6 Restful Backend w/ Unit Testing and ESDoc",
  "private": false,
  "engines": {
    "node": ">=0.12.x"
  },
  "dependencies": {
    "body-parser": "~1.13.3",
    "cors": "~2.7.1",
    "express": "~4.13.3"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/PranavSathy/node-backend-boilerplate"
  },
  "scripts": {
    "start": "node dist/app.js",
