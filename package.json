    "chai": "^3.2.0",
    "codacy-coverage": "^1.1.3",
    "del": "^2.0.2",
    "gulp": "^3.9.0",
    "gulp-apidoc": "^0.2.0",
    "gulp-babel": "^5.2.1",
    "gulp-esdoc": "0.0.4",
    "gulp-eslint": "^1.0.0",
    "istanbul": "^0.3.22",
    "mocha-lcov-reporter": "^1.0.0",
    "supertest": "^1.1.0"
  }
